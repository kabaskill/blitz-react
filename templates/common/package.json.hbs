{
  "name": "{{projectName}}",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "{{#if isTypeScript}}tsc && {{/if}}vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    {{#each dependencies}}
    "{{this.name}}": "^{{this.version}}"{{#unless @last}},{{/unless}}
    {{/each}}
  },
  "devDependencies": {
    {{#each devDependencies}}
    "{{this.name}}": "^{{this.version}}"{{#unless @last}},{{/unless}}
    {{/each}}
  }
}